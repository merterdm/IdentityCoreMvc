@model Citizen

<div class="card">
    <div class="card-header">
        <h3>@Model.first @Model.last</h3>
    </div>
    <div class="card-body">
        <table class="table table-active">
            <thead>
                <tr>
                    <td><label asp-for="@Model.first"></label></td>

                    <td><label asp-for="@Model.last"></label></td>
                    <td><label asp-for="@Model.fatherFirst"></label></td>
                    <td><label asp-for="@Model.motherFirst"></label></td>
                    <td><label asp-for="@Model.addressCity"></label></td>
                    <td><label asp-for="@Model.birthCity"></label></td>
                    <td><label asp-for="@Model.dateOfBirth"></label></td>
                    <td><label asp-for="@Model.gender"></label></td>
                    <td><label asp-for="@Model.streetAddress"></label></td>

                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><label>@Model.last</label></td>

                    <td><label>@Model.last</label></td>
                    <td><label>@Model.fatherFirst</label></td>
                    <td><label>@Model.motherFirst</label></td>
                    <td><label>@Model.addressCity</label></td>
                    <td><label>@Model.birthCity</label></td>
                    <td><label>@Model.dateOfBirth</label></td>
                    <td><label>@Model.gender</label></td>
                    <td><label>@Model.streetAddress</label></td>

                </tr>
            </tbody>

        </table>
    </div>
</div>
