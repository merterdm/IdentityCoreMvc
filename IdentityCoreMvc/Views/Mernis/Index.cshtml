@model ICollection<Citizen>


<div class="card">
    <div class="card-header">
        <h3>Mernis Listesi</h3>
    </div>
    <div class="card-body">
        <table class="table table-active">
            <thead>
                <tr>
                    <td>Tc no</td>
                    <td>Adi </td>
                    <td>Soyadi</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td><a asp-action="GetTcNo" asp-controller="Mernis" asp-route-id="@item.nationalIdentifier">@item.nationalIdentifier</a></td>
                        <td>@item.first</td>
                        <td>@item.last</td>
                    </tr>
                }
            </tbody>

        </table>
    </div>

</div>
